<h1>Previous Games</h1>
<ul>
  <% @rooms.each_with_index do |room, i| %>
    <h2><li><%= link_to "Game ##{i}", room_path(room)%></li></h2>
    <div>
      <h3><%="WIN" if room.won_by?(@user) %></h3>
      <h4>Finished on <%= room.updated_at%></h4>
      <h4>Played against <%= room.versus(@user).username%></h4>
    </div>
  <% end %>
</ul>
